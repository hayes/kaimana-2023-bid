{
  "name": "hanford-internet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm prepare && pnpm generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "prisma generate --data-proxy",
    "generate": "graphql-codegen",
    "dev": "concurrently \"pnpm next dev\" \"pnpm generate --watch './graphql/**/*.ts'\"",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\""
  },
  "prisma": {
    "seed": "pnpm ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
