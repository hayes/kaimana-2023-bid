    "@headlessui/react": "^1.7.3",
    "@heroicons/react": "^2.0.11",
    "@mantine/core": "^5.5.0",
    "@mantine/hooks": "^5.5.0",
    "@mantine/rte": "^5.5.0",
    "@pothos/core": "^3.22.5",
    "@pothos/plugin-prisma": "^3.35.4",
    "@pothos/plugin-simple-objects": "^3.6.4",
    "@prisma/client": "^4.4.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
